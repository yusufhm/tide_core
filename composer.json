            "_webform": true,
            "type": "package",
            "package": {
                "name": "ckeditor/image",
                "version": "4.16.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/image/releases/image_4.16.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "ckeditor.link": {
            "_webform": true,
            "type": "package",
            "package": {
                "name": "ckeditor/link",
                "version": "4.16.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/link/releases/link_4.16.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
